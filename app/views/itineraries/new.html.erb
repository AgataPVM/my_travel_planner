

<%= simple_form_for @itinerary do |f|%>
  <%= f.input :title, label: "Title"%>
  <%= f.input :destination, as: :string, label: "Destination" %>
  <%= f.input :days, as: :integer, label: "Number of days" %>
  <%= f.input :people, as: :integer, label: "Number of people" %>
  <%= f.input :interest, as: :text, label: "Interest" %>
  <%= f.button :submit, "Create Itinerary" %>
<% end %>
